BoxLayout:
    orientation: 'vertical'
    size:root.width,root.height


    MDToolbar:
        title: 'Demo Application'
        padding:10
        md_bg_color: rgba(0, 36, 106, 255)
        left_action_items: [["menu", lambda x: app.navigation_draw()]]
        right_action_items: [["dots-vertical", lambda x: print("Hey") if app.foo=="yay" else print("No")], ["clock", lambda x: app.callback_2()]]
        elevation:5



    MDBottomAppBar:
        MDToolbar:
            title: 'Demo'
            specific_text_color: app.theme_cls.accent_color
            icon_color: 0, 1, 0, 1
            icon: 'language-python'
            type: 'bottom'
            left_action_items: [["coffee", lambda x: app.navigation_draw()]]
            on_action_button: app.navigation_draw()
            mode: "end"


    MDLabel:
        text: 'hello world'
        halign:'center'
        font_style: 'H4'
        md_bg_color: rgba(154, 113, 217, 100)

    MDLabel:
        text: ''
        







